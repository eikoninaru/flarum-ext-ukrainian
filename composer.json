{
    "name": "eikoninaru/flarum-ext-ukrainian",
    "type": "flarum-extension",
    "description": "Ukrainian language pack for Flarum.",
    "homepage": "https://github.com/eikoninaru/flarum-ext-ukrainian",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Syzonov",
            "email": "root@linuxmint.com.ua",
            "homepage": "https://www.linuxmint.com.ua/"
        },
	{
     	    "name": "Igor Kiselyuk",
     	    "homepage": "http://netishin.com.ua/"
    	},
    	{   "name": "Dmytro Shovchko",
    	    "homepage": "https://github.com/dshovchko"
    	}
    ],
    "support": {
        "issues": "https://github.com/eikoninaru/flarum-ext-ukrainian/issues",
        "forum": "https://discuss.flarum.org/d/2614-ukrainian-language-pack"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "flarum-extension": {
            "title": "Ukrainian",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "uk",
            "title": "Українська"
        }
    }
}
